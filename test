(= next_state
    (if (= char #f0)
        then (if (= current state #f1)
                then #f2 
                else (if (= current state #f2)
                        then #f2 
                        else (if (= current state #f0)
                                then #f1 #f3))) 
        else #f3)




(= next_state
    (if (= char B)
        then (if (= current state #f1) 
                then #f2 
                else (if (= current state #f0) 
                        then #f2 
                        else (if (= current state #f2) 
                                then #f2 
                                else #f3))) 
        else (if (= char A) 
                  then (if (= current state #f0) 
                          then #f1 
                          else (if (= current state #f2) 
                                  then #f2 
                                  else (if (= current state #f1) 
                                      then #f2
                                      else #f3)))
                  else #f3)))


